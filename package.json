{
    "name": "neotable",
    "version": "0.4.0",
    "private": false,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build --target lib --name neo-table ./src/index.js",
        "lint": "vue-cli-service lint",
        "build-dist": "vue-cli-service build --target lib --name neo-table ./src/index.js"
    },
    "dependencies": {
        "lodash": "^4.17.20",
        "moment": "^2.29.1",
        "vue": "^2.6.12"
    },
    "devDependencies": {
        "vue-template-compiler": "^2.6.12",
        "bootstrap": "^4.6.0",
        "font-awesome": "^4.7.0"
    },
    "config": {
        "resolve": {
            "alias": {
                "vue$": "vue/dist/vue.esm.js"
            }
        }
    },
    "license": "MIT",
    "main": "./src/index.js",
    "files": [
        "dist/*",
        "src/index.js",
        "src/NeoTable.vue",
        "src/components/*",
        "src/lib/*"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bugsworld85/neotable.git"
    },
    "keywords": [
        "table",
        "data-table"
    ],
    "description": "Feature pack dynamic table.",
    "author": {
        "name": "Jovanni G",
        "email": "bugsworld85@gmail.com"
    }
}
